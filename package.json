{
    "name": "offdjs",
    "version": "2.0.0-1",
    "type": "module",
    "description": "oneki framework for discord.js",
    "main": "lib/index.js",
    "bin": "lib/cli.js",
    "scripts": {
        "prepare": "npm run build",
        "build": "tsc",
        "start": "node lib/index.js",
        "dev": "nodemon lib/index.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix"
    },
    "keywords": [
        "oneki",
        "djs",
        "discord.js",
        "discordjs",
        "discord-bot",
        "bot",
        "discord-api",
        "discord",
        "framework",
        "node",
        "nodejs",
        "node.js"
    ],
    "author": {
        "name": "eliyya",
        "email": "elimacmun@gmail.com",
        "url": "https://twitter.com/eli_yya"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/i18n": "0.13.6",
        "@types/node": "18.15.3",
        "standard": "^17.0.0",
        "typescript": "5.0.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/OnekiDevs/offdjs.git"
    },
    "bugs": {
        "url": "https://github.com/OnekiDevs/offdjs/issues"
    },
    "homepage": "https://github.com/OnekiDevs/offdjs#readme",
    "dependencies": {
        "discord.js": "14.11.0",
        "dotenv": "16.0.3",
        "i18n": "0.15.1",
        "just-extend": "6.2.0"
    },
    "engines": {
        "node": ">=18.12.0"
    },
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4,
        "useTabs": false,
        "semi": false,
        "singleQuote": true,
        "quoteProps": "as-needed",
        "trailingComma": "all",
        "arrowParens": "avoid"
    },
    "eslintConfig": {
        "extends": "./node_modules/standard/eslintrc.json"
    }
}
